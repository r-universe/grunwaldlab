[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/cran-to-git
	shallow = true
	branch = HEAD
[submodule "ezec"]
	path = ezec
	url = https://github.com/grunwaldlab/ezec
	shallow = true
	registered = true
	oncran = true
